{
  "name": "@fullstackhouse/nestjs-outbox-mikro-orm-driver",
  "version": "3.0.0",
  "description": "This package provides a driver for the NestJS Outbox library that uses MikroORM to interact with the database.",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:benchmark": "vitest run --testNamePattern='Benchmark'",
    "benchmark": "BENCHMARK_OUTPUT_PATH=./benchmark-results.json vitest run --testNamePattern='Benchmark'",
    "tsc:publish": "tsc && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstackhouse/nestjs-outbox.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Full Stack House",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@fullstackhouse/nestjs-outbox": "^3.0.0",
    "@mikro-orm/core": "^6.3.9",
    "@mikro-orm/migrations": "^6.3.9"
  },
  "devDependencies": {
    "@mikro-orm/mysql": "^6.3.9",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/postgresql": "^6.3.9",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@swc/core": "^1.7.42",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "mysql2": "^3.11.0",
    "pg": "^8.13.1",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typescript": "^5.6.2",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.1"
  },
  "gitHead": "010c167e60c596cb1aabf28186242a095c351e59"
}
